{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "ec2:AcceptVpcPeeringConnection",
            "Resource": [
                "arn:aws:ec2:*:*:vpc-peering-connection/*",
                "arn:aws:ec2:*:*:vpc/*"
            ]
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "ec2:CreateVpnConnectionRoute",
                "ec2:DescribeRouteTables",
                "ec2:AssociateRouteTable",
                "ec2:CreateRoute",
                "ec2:EnableVgwRoutePropagation"
            ],
            "Resource": "*"
        }
    ]
}